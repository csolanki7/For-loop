package forloopexample;

public class armstrongnumber {

	public static void main (String [] args ){
		int n = 153; 
		int temp =n ; 
		int sum=0,c; 
		
		while (n>0) {
			
			c=n%10; 
		    n=n/10; 
		    sum = sum + c*c*c;
		    
		}
		
		if(temp== sum){
			
			System.out.println("is amstrong ");
		}
		else {
			
			System.out.print("is not amstrong");
		}
		
	}
}
